{
  "components": ["devices/SF-884/SF-884-0.1.js"],
  "targetDirectory": "scripts/devices",
  "config":{
		"debug": false,
		"disabled": true,
		"portName": '?',
		"baudRate": 9600,
		"dataBits": 8,
		"parity": 0,
		"stopBits": 1,
		"delimiters": 0,
		"convertToHex*": true,
		"regexp*": '(%d+%.%d+)',
		"responseLen": 27,
		"simulate": '1.5',
		"scripts*": {
			"onInit": {
				"file": "scripts\\devices\\SF-884\\SF-884 init.js"
			},
			"onRequest: {
				"file": "scripts\\devices\\SF-884\\SF-884 init.js"
			},
			"onReceive": {
				"file": "scripts\\devices\\SF-884\\SF-884 init.js"
			}
		}
	}
}
